{
  "name": "@ovh/ui-kit",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/js/oui.js",
  "style": "./dist/css/oui.css",
  "scripts": {
    "build": "npm-run-all -l clean -p build:* -s copy",
    "build:css": "lessc --remcalc --clean-css --source-map ./src/less/oui.less ./dist/css/oui.css",
    "build:fonts": "cpx \"./src/fonts/**/*.{eot,svg,ttf,woff,woff2}\" \"./dist/fonts\"",
    "build:icons": "mkdirp ./dist/fonts/oui-icons && npm run build:icons:generator && node ./src/js/generate-icons-indexes.js",
    "build:icons:generator": "icon-font-generator ./src/svg/icons/*.svg -n oui-icons -p oui-icon -o ./dist/fonts/oui-icons -t span --types 'woff, woff2'",
    "build:js": "webpack --progress --colors --config ./.config/webpack.dist.config.js",
    "build:less": "run-p -l build:less:*",
    "build:less:constants": "lessc ./src/less/_constants.less ./dist/less/_constants.less",
    "build:less:icons": "lessc ./src/less/_icons.less ./dist/less/_icons.less",
    "build:less:variables": "lessc ./src/less/_variables.less ./dist/less/_variables.less",
    "clean": "rimraf ./dist",
    "copy": "cpx \"./dist/**/*\" \"../../../dist\"",
    "prepare": "yarn build",
    "version": "cross-conf-env sed -e 's/${tmpl-npm-version}/npm_package_version/' ./src/less/_version.tmpl.less > ./src/less/_version.less",
    "watch": "npm-run-all -l clean -p watch:* -s",
    "watch:css": "run-s -l watch:css:*",
    "watch:css:build": "yarn run build:css",
    "watch:css:watch": "less-watch-compiler --plugins remcalc ./src/less ./dist/css oui.less",
    "watch:fonts": "npm run build:fonts",
    "watch:icons": "npm run build:icons",
    "watch:js": "webpack --progress --colors --config ./.config/webpack.dev.config.js",
    "watch:less": "npm run build:less"
  },
  "dependencies": {
    "bloodhound-js": "^1.2.3",
    "clipboard": "^2.0.1",
    "flatpickr": "^4.5.2",
    "popper.js": "^1.14.4",
    "ui-select": "^0.19.8"
  },
  "peerDependencies": {
    "angular": ">=1.6.x",
    "angular-aria": ">=1.6.x",
    "angular-mocks": ">=1.6.x",
    "angular-sanitize": ">=1.6.x"
  },
  "devDependencies": {
    "@ovh/ui-kit.action-menu": "*",
    "@ovh/ui-kit.back-button": "*",
    "@ovh/ui-kit.button": "*",
    "@ovh/ui-kit.calendar": "*",
    "@ovh/ui-kit.checkbox": "*",
    "@ovh/ui-kit.chips": "*",
    "@ovh/ui-kit.clipboard": "*",
    "@ovh/ui-kit.collapsible": "*",
    "@ovh/ui-kit.criteria": "*",
    "@ovh/ui-kit.datagrid": "*",
    "@ovh/ui-kit.dropdown": "*",
    "@ovh/ui-kit.field": "*",
    "@ovh/ui-kit.form-actions": "*",
    "@ovh/ui-kit.guide-menu": "*",
    "@ovh/ui-kit.header-tabs": "*",
    "@ovh/ui-kit.message": "*",
    "@ovh/ui-kit.modal": "*",
    "@ovh/ui-kit.navbar": "*",
    "@ovh/ui-kit.numeric": "*",
    "@ovh/ui-kit.page-header": "*",
    "@ovh/ui-kit.pagination": "*",
    "@ovh/ui-kit.popover": "*",
    "@ovh/ui-kit.progress": "*",
    "@ovh/ui-kit.radio": "*",
    "@ovh/ui-kit.search": "*",
    "@ovh/ui-kit.select": "*",
    "@ovh/ui-kit.select-picker": "*",
    "@ovh/ui-kit.skeleton": "*",
    "@ovh/ui-kit.slideshow": "*",
    "@ovh/ui-kit.spinner": "*",
    "@ovh/ui-kit.stepper": "*",
    "@ovh/ui-kit.switch": "*",
    "@ovh/ui-kit.textarea": "*",
    "@ovh/ui-kit.tile": "*",
    "@ovh/ui-kit.tooltip": "*",
    "babel-loader": "^7.1.5",
    "angular": "^1.7.6",
    "angular-aria": "^1.7.6",
    "angular-mocks": "^1.7.6",
    "angular-sanitize": "^1.7.6",
    "chalk": "^2.4.1",
    "cpx": "^1.5.0",
    "cross-conf-env": "^1.1.2",
    "eslint-loader": "^2.1.1",
    "html-loader": "^0.5.5",
    "icon-font-generator": "^2.1.9",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^4.0.0-rc.2",
    "less": "^3.8.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-remcalc": "^0.1.0",
    "less-watch-compiler": "^1.13.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.24.0",
    "webpack-merge": "^4.1.4"
  }
}
