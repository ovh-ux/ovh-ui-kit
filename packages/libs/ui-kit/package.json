    "@ovh/ui-kit.clipboard": "*",
    "@ovh/ui-kit.collapsible": "*",
    "@ovh/ui-kit.criteria": "*",
    "@ovh/ui-kit.datagrid": "*",
    "@ovh/ui-kit.dropdown": "*",
    "@ovh/ui-kit.field": "*",
    "@ovh/ui-kit.form-actions": "*",
    "@ovh/ui-kit.guide-menu": "*",
    "@ovh/ui-kit.header-tabs": "*",
