{
  "name": "@ovh/ui-kit",
  "version": "0.0.0",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/js/oui.js",
  "style": "./dist/css/oui.css",
  "scripts": {
    "build": "npm-run-all -l -p build:* -s copy",
    "build:less": "run-p -l build:less:*",
    "build:less:constants": "lessc ./src/less/_constants.less ./dist/less/_constants.less",
    "build:less:icons": "lessc ./src/less/_icons.less ./dist/less/_icons.less",
    "build:less:variables": "lessc ./src/less/_variables.less ./dist/less/_variables.less",
    "build:webpack": "webpack --progress --colors --config ./.config/webpack.dist.config.js",
    "clean": "rimraf ./dist",
    "copy": "cpx \"./dist/**/*\" \"../../../dist\"",
    "prepare": "yarn build",
    "version": "cross-conf-env sed -e 's/${tmpl-npm-version}/npm_package_version/' ./src/less/_version.tmpl.less > ./src/less/_version.less",
    "watch": "npm-run-all -l clean -p watch:* -s",
    "watch:less": "npm run build:less",
    "watch:webpack": "webpack --progress --colors --config ./.config/webpack.dev.config.js"
  },
  "dependencies": {
    "bloodhound-js": "^1.2.3",
    "clipboard": "^2.0.1",
    "flatpickr": "^4.5.2",
    "popper.js": "^1.14.4",
    "ui-select": "^0.19.8"
  },
  "peerDependencies": {
    "angular": ">=1.6.x",
    "angular-aria": ">=1.6.x",
    "angular-mocks": ">=1.6.x",
    "angular-sanitize": ">=1.6.x"
  },
