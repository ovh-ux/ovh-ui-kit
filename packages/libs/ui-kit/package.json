{
  "name": "@ovh/ui-kit",
  "version": "3.0.0-alpha",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/js/oui.min.js",
  "style": "./dist/css/oui.min.css",
  "scripts": {
    "build": "run-s clean version css js fonts icons less",
    "clean": "rimraf ./dist",
    "css": "run-p \"css:*\"",
    "css:debug": "lessc --remcalc ./src/less/index.less ./dist/css/oui.css",
    "css:min": "lessc --remcalc --clean-css --source-map ./src/less/index.less ./dist/css/oui.min.css",
    "fonts": "cpx \"./src/fonts/**/*.{eot,svg,ttf,woff,woff2}\" \"./dist/fonts\"",
    "icons": "mkdirp ./dist/fonts/oui-icons && npm run icons:generator && node ./scripts/generate-icons-indexes.js",
    "icons:generator": "icon-font-generator ./src/svg/icons/*.svg -n oui-icons -p oui-icon -o ./dist/fonts/oui-icons -t span --types 'woff, woff2'",
    "js": "webpack --progress --colors --config ./config/webpack.dist.config.js",
    "less": "run-p \"less:*\"",
    "less:constants": "lessc ./src/less/_constants.less ./dist/less/_constants.less",
    "less:icons": "lessc ./src/less/_icons.less ./dist/less/_icons.less",
    "less:variables": "lessc ./src/less/_variables.less ./dist/less/_variables.less",
    "version": "cross-conf-env sed -e 's/${tmpl-npm-version}/npm_package_version/' ./src/less/_version.tmpl.less > ./src/less/_version.less"
  },
  "dependencies": {
    "bloodhound-js": "^1.2.3",
    "clipboard": "^2.0.1",
    "flatpickr": "^4.5.2",
    "popper.js": "^1.14.4",
    "ui-select": "^0.19.8"
  },
  "peerDependencies": {
    "angular": ">=1.6.x",
    "angular-aria": ">=1.6.x",
    "angular-mocks": ">=1.6.x",
    "angular-sanitize": ">=1.6.x"
  },
  "devDependencies": {
    "@ovh/ui-kit.action-menu": "*",
    "@ovh/ui-kit.back-button": "*",
    "@ovh/ui-kit.button": "*",
    "@ovh/ui-kit.calendar": "*",
    "@ovh/ui-kit.checkbox": "*",
    "@ovh/ui-kit.chips": "*",
    "@ovh/ui-kit.clipboard": "*",
    "@ovh/ui-kit.collapsible": "*",
    "@ovh/ui-kit.criteria": "*",
    "@ovh/ui-kit.datagrid": "*",
    "@ovh/ui-kit.dropdown": "*",
    "@ovh/ui-kit.field": "*",
    "@ovh/ui-kit.form-actions": "*",
    "@ovh/ui-kit.guide-menu": "*",
    "@ovh/ui-kit.header-tabs": "*",
    "@ovh/ui-kit.message": "*",
    "@ovh/ui-kit.modal": "*",
    "@ovh/ui-kit.navbar": "*",
    "@ovh/ui-kit.numeric": "*",
    "@ovh/ui-kit.page-header": "*",
    "@ovh/ui-kit.pagination": "*",
    "@ovh/ui-kit.popover": "*",
    "@ovh/ui-kit.progress": "*",
    "@ovh/ui-kit.radio": "*",
    "@ovh/ui-kit.search": "*",
    "@ovh/ui-kit.select": "*",
    "@ovh/ui-kit.select-picker": "*",
    "@ovh/ui-kit.skeleton": "*",
    "@ovh/ui-kit.slideshow": "*",
    "@ovh/ui-kit.spinner": "*",
    "@ovh/ui-kit.stepper": "*",
    "@ovh/ui-kit.switch": "*",
    "@ovh/ui-kit.textarea": "*",
    "@ovh/ui-kit.tile": "*",
    "@ovh/ui-kit.tooltip": "*",
    "chalk": "^2.4.1",
    "cpx": "^1.5.0",
    "cross-conf-env": "^1.1.2",
    "icon-font-generator": "^2.1.9",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2"
  }
}
