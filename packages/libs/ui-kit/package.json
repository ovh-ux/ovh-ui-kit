  "main": "./dist/js/oui.js",
  "style": "./dist/css/oui.css",
  "scripts": {
    "build": "npm-run-all -l -s build:* copy",
    "build:less": "run-p build:less:*",
    "build:less:constants": "lessc ./src/less/_constants.less ./dist/less/_constants.less",
    "build:less:icons": "lessc ./src/less/_icons.less ./dist/less/_icons.less",
    "build:less:variables": "lessc ./src/less/_variables.less ./dist/less/_variables.less",
    "build:webpack": "webpack --progress --colors --config ./.config/webpack.config.js",
    "build-storybook": "build-storybook",
    "clean": "rimraf ./dist",
    "copy": "cpx \"./dist/**/*\" \"../../../dist\"",
    "prepare": "yarn build",
    "storybook": "start-storybook",
    "version": "cross-conf-env sed -e 's/${tmpl-npm-version}/npm_package_version/' ./src/less/_version.tmpl.less > ./src/less/_version.less"
  },
