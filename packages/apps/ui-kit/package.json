{
  "name": "@ovh/ui-kit",
  "version": "3.0.0",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "main": "./dist/js/oui.min.js",
  "style": "./dist/css/oui.min.css",
  "scripts": {
    "build": "npm run build:clean && npm run build:version && npm run build:fonts && npm run build:icons && npm run build:js && npm run build:less && npm run build:css",
    "build:clean": "rimraf ./dist",
    "build:fonts": "cpx \"../../components/_core/fonts/**/*.{eot,svg,ttf,woff,woff2}\" \"./dist/fonts\"",
    "build:icons": "mkdirp ./dist/fonts/oui-icons && npm run build:icons-generator && node ./scripts/generate-icons-indexes.js",
    "build:icons-generator": "icon-font-generator ../../components/_core/svg/icons/*.svg -n oui-icons -p oui-icon -o ./dist/fonts/oui-icons -t span --types 'woff, woff2'",
    "build:js": "webpack --progress --colors --config ./config/webpack.dist.config.js",
    "build:css": "npm run build:css-min && npm run build:css-debug",
    "build:css-min": "lessc --remcalc --clean-css --source-map ./src/less/_dist.less ./dist/css/oui.min.css",
    "build:css-debug": "lessc --remcalc ./src/less/_dist.less ./dist/css/oui.css",
    "build:less": "npm run build:less-constants && npm run build:less-icons && npm run build:less-variables",
    "build:less-constants": "lessc ./src/less/_constants.less ./dist/less/_constants.less",
    "build:less-icons": "lessc ./src/less/_icons.less ./dist/less/_icons.less",
    "build:less-variables": "lessc ./src/less/_variables.less ./dist/less/_variables.less",
    "build:version": "cross-conf-env sed -e 's/${tmpl-npm-version}/npm_package_version/' ./src/less/_version.tmpl.less > ./src/less/_version.less"
  },
  "dependencies": {
    "bloodhound-js": "^1.2.3",
    "clipboard": "^2.0.1",
    "flatpickr": "^4.5.2",
    "popper.js": "^1.14.4",
    "ui-select": "^0.19.8"
  },
  "peerDependencies": {
    "angular": ">=1.6.x",
    "angular-aria": ">=1.6.x",
    "angular-mocks": ">=1.6.x",
    "angular-sanitize": ">=1.6.x"
  },
  "devDependencies": {
    "@ovh/ui-kit.action-menu": "^3.0.0",
    "@ovh/ui-kit.back-button": "^3.0.0",
    "@ovh/ui-kit.button": "^3.0.0",
    "@ovh/ui-kit.calendar": "^3.0.0",
    "@ovh/ui-kit.checkbox": "^3.0.0",
    "@ovh/ui-kit.chips": "^3.0.0",
    "@ovh/ui-kit.clipboard": "^3.0.0",
    "@ovh/ui-kit.collapsible": "^3.0.0",
    "@ovh/ui-kit.criteria": "^3.0.0",
    "@ovh/ui-kit.datagrid": "^3.0.0",
    "@ovh/ui-kit.dropdown": "^3.0.0",
    "@ovh/ui-kit.field": "^3.0.0",
    "@ovh/ui-kit.form-actions": "^3.0.0",
    "@ovh/ui-kit.guide-menu": "^3.0.0",
    "@ovh/ui-kit.header-tabs": "^3.0.0",
    "@ovh/ui-kit.message": "^3.0.0",
    "@ovh/ui-kit.modal": "^3.0.0",
    "@ovh/ui-kit.navbar": "^3.0.0",
    "@ovh/ui-kit.numeric": "^3.0.0",
    "@ovh/ui-kit.page-header": "^3.0.0",
    "@ovh/ui-kit.pagination": "^3.0.0",
    "@ovh/ui-kit.popover": "^3.0.0",
    "@ovh/ui-kit.progress": "^3.0.0",
    "@ovh/ui-kit.radio": "^3.0.0",
    "@ovh/ui-kit.search": "^3.0.0",
    "@ovh/ui-kit.select": "^3.0.0",
    "@ovh/ui-kit.select-picker": "^3.0.0",
    "@ovh/ui-kit.skeleton": "^3.0.0",
    "@ovh/ui-kit.slideshow": "^3.0.0",
    "@ovh/ui-kit.spinner": "^3.0.0",
    "@ovh/ui-kit.stepper": "^3.0.0",
    "@ovh/ui-kit.switch": "^3.0.0",
    "@ovh/ui-kit.textarea": "^3.0.0",
    "@ovh/ui-kit.tile": "^3.0.0",
    "@ovh/ui-kit.tooltip": "^3.0.0",
    "chalk": "^2.4.1",
    "cpx": "^1.5.0",
    "cross-conf-env": "^1.1.2",
    "icon-font-generator": "^2.1.9",
    "mkdirp": "^0.5.1",
    "rimraf": "^2.6.2"
  }
}
