{
  "name": "@ovh/ui-kit-workshop",
  "private": true,
  "version": "3.0.0",
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "scripts": {
    "build": "npm-run-all -l -s clean build-storybook copy",
    "build-storybook": "build-storybook -c .storybook -o docs",
    "clean": "rimraf ./docs",
    "copy": "cpx \"./docs/**/*\" \"../../../docs\"",
    "serve": "npx serve ./docs",
    "start": "yarn storybook",
    "storybook": "start-storybook"
  },
  "dependencies": {
    "@ovh/ui-kit": "*",
    "angular": "^1.7.8",
    "angular-aria": "^1.7.8",
    "angular-sanitize": "^1.7.8"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@ovh/ui-kit.config": "*",
    "@storybook/addon-a11y": "^5.0.6",
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-storysource": "^5.0.6",
    "@storybook/cli": "^5.0.6",
    "@storybook/html": "^5.0.6",
    "babel-loader": "^8.0.5",
    "cpx": "^1.5.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "storybook-addon-angularjs": "^0.0.2"
  }
}
