{
  "name": "oui-less",
  "version": "0.0.0",
  "description": "A stylekit for the OVH brand (Less).",
  "license": "UNLICENSED",
  "scripts": {
    "build": "lessc --remcalc packages/oui/stylekit.less dist/oui.css",
    "fix": "stylefmt -r packages",
    "lint": "stylelint --syntax less 'packages/**/*.less'",
    "preversion": "npm build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "less-plugin-remcalc": "git+ssh://git@stash.ovh.net:7999/UXTOOLS/less-plugin-remcalc.git"
  },
  "devDependencies": {
    "less": "^2.7.2",
    "stylefmt": "^5.0.3",
    "stylelint": "^7.7.1"
  }
}
